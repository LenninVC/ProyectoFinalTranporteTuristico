@model IEnumerable<Cibertec.Models.Reserva>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdReserva)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Itinerario)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.IdItinerario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdCliente)*@
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellidos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Inicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Fin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Costo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdReserva)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Itinerario)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.IdItinerario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdCliente)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Inicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Fin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costo)
        </td>
        <td>
            <a href="#" onclick="cibertec.getModal('@Url.Action("Update", new { id = item.IdReserva })');
                       return false;" data-toggle="modal" data-target="#modal-container"
               class="btn btn-warning btn-sm">
                <span class="glyphicon glyphicon-edit"> Edit </span>
            </a>
            <a href="#" onclick="cibertec.getModal('@Url.Action("Delete", new { id = item.IdReserva })');
                       return false;" data-toggle="modal" data-target="#modal-container"
               class="btn btn-danger btn-sm">
                <span class="glyphicon glyphicon-trash"> Delete </span>
            </a>
        </td>
    </tr>
}

</table>
